keywords = {Animal,Animal: epidemiology,Animal: parasitology,Animals,Binomial Distribution,Host-Parasite Interactions,Host-Parasite Interactions: physiology,Parasites,Parasites: isolation \& purification,Parasites: physiology,Parasitic Diseases,Poisson Distribution,Prevalence,Seasons,Wild,Wild: parasitology},
