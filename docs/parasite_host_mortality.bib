@article{Lafferty2002,
abstract = {A primary difference between predators and parasites is the number of victims that an individual attacks throughout a life-history stage. A key division within natural enemies is whether a successful attack eliminates the fitness of the prey or the host. A third distinctive axis for parasites is whether the host must die to further parasite development. The presence or absence of intensity-dependent pathology is a fourth factor that separates macroparasites from microparasites; this also distinguishes between social and solitary predators. Combining these four dichotomies defines seven types of parasitism, seven corresponding parasites, three forms of predation and, when one considers obligate and facultative combinations of these forms, four types of predator. Here, we argue that the energetics underlying the relative and absolute sizes of natural enemies and their victims is the primary selective factor responsible for the evolution of these different trophic strategies.},
author = {Lafferty, Kevin D. and Kuris, Armand M.},
doi = {10.1016/S0169-5347(02)02615-0},
file = {:Users/mqwilber/Dropbox/Documents/Papers\_and\_Books/LaffertyandKuris2002.pdf:pdf},
isbn = {0169-5347},
issn = {01695347},
journal = {Trends in Ecology and Evolution},
number = {11},
pages = {507--513},
pmid = {444},
title = {{Trophic strategies, animal diversity and body size}},
volume = {17},
year = {2002}
}
